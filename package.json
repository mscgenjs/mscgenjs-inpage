{
  "name": "mscgenjs-inpage",
  "version": "2.0.4",
  "description": "Embed and render sequence charts in HTML",
  "main": "dist/mscgen-inpage.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mscgenjs/mscgenjs-inpage"
  },
  "author": "Sander Verweij",
  "license": "GPL-3.0",
  "keywords": [
    "mscgen",
    "embed",
    "sequence chart",
    "sequence diagram",
    "xu",
    "msgenny"
  ],
  "bugs": {
    "url": "https://github.com/mscgenjs/mscgenjs-inpage/issues"
  },
  "homepage": "https://github.com/mscgenjs/mscgenjs-inpage",
  "files": [
    "dist/mscgen-inpage.js",
    "package.json",
    "README.md",
    "COPYING"
  ],
  "dependencies": {
    "mscgenjs": "3.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "dependency-cruiser": "4.6.3",
    "eslint": "5.9.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "4.0.0",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.5",
    "nyc": "13.1.0",
    "upem": "1.0.2",
    "webpack": "4.26.1",
    "webpack-cli": "3.1.2"
  },
  "scripts": {
    "build": "webpack",
    "check": "run-p depcruise lint test:cover",
    "depcruise": "depcruise -v -- src test",
    "depcruise:graph": "depcruise -T rcdot -v -- src | dot -T svg > tmp_deps.svg",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "scm:stage": "git add .",
    "test": "mocha --recursive test --invert --grep \"\\(browser only\\)\"",
    "test:cover": "nyc --check-coverage npm test",
    "update-dependencies": "run-s upem:update upem:install lint:fix check",
    "upem:update": "npm outdated --json | upem",
    "upem:install": "npm install",
    "version": "run-s build scm:stage"
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "security"
    ],
    "env": {
      "browser": true
    },
    "overrides": [
      {
        "files": [
          "test/**/*.js"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "dist",
    "node_modules",
    "coverage",
    "wikum"
  ],
  "nyc": {
    "statements": 80,
    "branches": 80,
    "functions": 85,
    "lines": 80,
    "exclude": [
      "coverage",
      "dist",
      "node_modules",
      "webpack.config.js",
      "test",
      "wikum"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "all": false
  }
}
