{
  "name": "mscgenjs-inpage",
  "version": "1.12.7",
  "description": "Embed and render MscGen in HTML",
  "main": "dist/mscgen-inpage.js",
  "dependencies": {},
  "devDependencies": {
    "almond": "0.3.3",
    "amdefine": "1.0.1",
    "chai": "4.1.2",
    "eslint": "4.11.0",
    "istanbul": "0.4.5",
    "js-makedepend": "2.4.0",
    "mocha": "3.5.3",
    "mscgenjs": "1.12.6",
    "npm-check-updates": "2.13.0",
    "nsp": "3.1.0",
    "plato": "1.7.0",
    "requirejs": "2.3.5"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "npm-check-updates": "ncu -x mocha --upgrade",
    "nsp": "nsp check",
    "plato": "plato -r -d platoreports -x \"parser|test|lib\" src/",
    "postversion": "git push && git push --tags && npm publish",
    "preversion": "test `git branch | grep \"^* [a-zA-Z]\" | cut -c 3-` = 'master'",
    "test": "mocha -R dot --recursive src/test --invert --grep \"\\(browser only\\)\"",
    "test:cover": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive src/test --invert --grep \"\\(browser only\\)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mscgenjs/mscgenjs-inpage"
  },
  "author": "Sander Verweij",
  "license": "GPL-3.0",
  "keywords": [
    "mscgen",
    "embed",
    "sequence chart",
    "sequence diagram",
    "xu",
    "msgenny"
  ],
  "bugs": {
    "url": "https://github.com/mscgenjs/mscgenjs-inpage/issues"
  },
  "homepage": "https://github.com/mscgenjs/mscgenjs-inpage"
}
