{
  "name": "mscgenjs-inpage",
  "version": "2.0.2",
  "description": "Embed and render sequence charts in HTML",
  "main": "dist/mscgen-inpage.js",
  "files": [
    "dist/mscgen-inpage.js",
    "package.json",
    "README.md",
    "COPYING"
  ],
  "dependencies": {
    "mscgenjs": "3.0.0-beta-2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "dependency-cruiser": "4.6.1",
    "eslint": "5.7.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-security": "1.4.0",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "upem": "1.0.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2"
  },
  "scripts": {
    "build": "webpack",
    "check": "npm-run-all --parallel depcruise lint test:cover",
    "depcruise": "depcruise -v -- src test",
    "depcruise:graph": "depcruise -T dot -v -- src | dot -T svg > tmp_deps.svg",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "preversion": "test `git branch | grep \"^* [a-zA-Z]\" | cut -c 3-` = 'master' && npm run build",
    "test": "mocha --recursive test --invert --grep \"\\(browser only\\)\"",
    "test:cover": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive test --invert --grep \"\\(browser only\\)\"",
    "update-dependencies": "npm-run-all upem:update upem:install lint:fix check",
    "upem:update": "npm outdated --json | upem",
    "upem:install": "npm install"
  },
  "upem": {
    "donotup": [
      "mscgenjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mscgenjs/mscgenjs-inpage"
  },
  "author": "Sander Verweij",
  "license": "GPL-3.0",
  "keywords": [
    "mscgen",
    "embed",
    "sequence chart",
    "sequence diagram",
    "xu",
    "msgenny"
  ],
  "bugs": {
    "url": "https://github.com/mscgenjs/mscgenjs-inpage/issues"
  },
  "homepage": "https://github.com/mscgenjs/mscgenjs-inpage"
}
